@model HelpDesk.Web.Models.AssetsDTO

<style>
    .all-details-view .form-group {
        margin-bottom: 5px;
    }
</style>
@if (Model.RoleId == 501 && Model.EditMode == true)
{
    <div class="kt-container  kt-grid__item kt-grid__item--fluid mt-4 position-relative">
        <a href="#" onclick="closeDetails()" class="details-close-button btn btn-danger btn-sm btn-icon btn-circle position-absolute"><i class="la la-close"></i></a>
        <!--Begin::Dashboard 3-->
        <!--Begin::Row-->
        <div class="row">
            <div class="col-lg-12 col-xl-12 order-lg-1 order-xl-1">
                <div class="kt-portlet kt-portlet--head--noborder kt-portlet--height-fluid">
                    <div class="kt-portlet__head kt-portlet__head--lg">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon">
                                <i class="kt-font-brand flaticon2-line-chart"></i>
                            </span>
                            <h3 class="kt-portlet__head-title">
                                Approval
                            </h3>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            <div class="kt-portlet__head-wrapper">
                                @if (Model.RoleId == 501)
                                {

                                    <a href="#" onclick="AssetApproval(1)" class="btn btn-brand btn-elevate btn-icon-sm">
                                        <i class="la la-check"></i>
                                        Approve
                                    </a>

                                    <a href="#" onclick="AssetApproval(2)" class="btn btn-danger btn-elevate btn-icon-sm">
                                        <i class="la la-close"></i>
                                        Reject
                                    </a>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="kt-portlet__body kt-portlet__body--fluid kt-portlet__body--fit">
                        <div class="kt-container  kt-grid__item kt-grid__item--fluid">
                            <div class="row">
                                <div class="col-md-12 pt-3 d-flex justify-content-between">
                                    <h4><small>Updated By : </small>@Model.UpdateUserName</h4>
                                    <h4><small>Resquested On : </small>@Model.ModifiedOn.ToString("dd-MM-yyyy HH:mm")</h4>
                                </div>
                                <div class="col-md-12">
                                    <div class="kt-form kt-form--label-right">
                                        <div class="kt-portlet__body all-details-view">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="table-responsive">
                                                        <table class="table table-bordered">
                                                            <tr>
                                                                <td>Product</td>
                                                                <td>@Model.ProductName </td>
                                                            </tr>

                                                            <tr>
                                                                <td>Region</td>
                                                                <td>@Model.RegionName </td>
                                                            </tr>

                                                            <tr>
                                                                <td>City</td>
                                                                <td>@Model.CityName </td>
                                                            </tr>

                                                            <tr>
                                                                <td>Station Name</td>
                                                                <td>@Model.StationName </td>
                                                            </tr>

                                                            <tr>
                                                                <td>Location</td>
                                                                <td>@Model.Area </td>
                                                            </tr>
                                                            <tr>
                                                                <td>Installation Date</td>
                                                                <td>@Model.InstallationDate.ToString("dd/MM/yyyy") </td>
                                                            </tr>

                                                            <tr>
                                                                <td>Warranty Expired Date</td>
                                                                <td>@Model.WarrantyExpiryDate.ToString("dd/MM/yyyy") </td>
                                                            </tr>
                                                            <tr>
                                                                <td>POContract</td>
                                                                <td>@Model.POContract </td>
                                                            </tr>
                                                        </table>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="table-responsive">
                                                        <table class="table table-bordered">
                                                            @if (Model.UpdatedList != null)
                                                            {
                                                                foreach (var item in Model.UpdatedList)
                                                                {
                                                                    if (Model.ProductName == item.ProductName)
                                                                    {
                                                                        <tr>
                                                                            <td>Product</td>
                                                                            <td>@item.ProductName </td>
                                                                        </tr>

                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td>Product</td>
                                                                            <td>@item.ProductName </td>
                                                                        </tr>
                                                                    }

                                                                    if (Model.RegionName == item.RegionName)
                                                                    {
                                                                        <tr>
                                                                            <td>Region</td>
                                                                            <td>@item.RegionName </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td>Region</td>
                                                                            <td>@item.RegionName </td>
                                                                        </tr>
                                                                    }

                                                                    if (Model.CityName == item.CityName)
                                                                    {
                                                                        <tr>
                                                                            <td>City</td>
                                                                            <td>@item.CityName </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td>City</td>
                                                                            <td>@item.CityName </td>
                                                                        </tr>
                                                                    }
                                                                    if (Model.StationName == item.StationName)
                                                                    {
                                                                        <tr>
                                                                            <td>Station Name</td>
                                                                            <td>@item.StationName </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td>Station Name</td>
                                                                            <td>@item.StationName </td>
                                                                        </tr>
                                                                    }

                                                                    if (Model.Area == item.Area)
                                                                    {
                                                                        <tr>
                                                                            <td>Location</td>
                                                                            <td>@item.Area </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td> Location</td>
                                                                            <td>@item.Area </td>
                                                                        </tr>
                                                                    }
                                                                    if (Model.InstallationDate == item.InstallationDate)
                                                                    {
                                                                        <tr>
                                                                            <td>Installation Date</td>
                                                                            <td>@item.InstallationDate.ToString("dd/MM/yyyy") </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td> Installation Date</td>
                                                                            <td>@item.InstallationDate.ToString("dd/MM/yyyy") </td>
                                                                        </tr>
                                                                    }
                                                                    if (Model.WarrantyExpiryDate == item.WarrantyExpiryDate)
                                                                    {
                                                                        <tr>
                                                                            <td>WarrantyExpiryDate Date</td>
                                                                            <td>@item.WarrantyExpiryDate.ToString("dd/MM/yyyy") </td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td> WarrantyExpiryDate Date</td>
                                                                            <td>@item.WarrantyExpiryDate.ToString("dd/MM/yyyy") </td>
                                                                        </tr>
                                                                    }
                                                                    if (Model.POContract == item.POContract)
                                                                    {
                                                                        <tr>
                                                                            <td>POContract</td>
                                                                            <td>@item.POContract</td>
                                                                        </tr>
                                                                    }
                                                                    else
                                                                    {
                                                                        <tr class="bg-light">
                                                                            <td> POContract</td>
                                                                            <td>@item.POContract </td>
                                                                        </tr>
                                                                    }
                                                                }
                                                            }
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
@if (Model.RoleId == 502 || Model.RoleId == 501)
{
    <div class="kt-container  kt-grid__item kt-grid__item--fluid mt-4">
        <!--Begin::Dashboard 3-->
        <!--Begin::Row-->
        <div class="row">
            <div class="col-lg-12 col-xl-12 order-lg-1 order-xl-1">
                <div class="kt-portlet kt-portlet--head--noborder kt-portlet--height-fluid">
                    <div class="kt-portlet__head kt-portlet__head--lg">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon">
                                <i class="kt-font-brand flaticon2-line-chart"></i>
                            </span>
                            <h3 class="kt-portlet__head-title">
                                Non-Editable
                            </h3>
                        </div>
                        @if (Model.RoleId == 501 && Model.EditMode == true)
                        {

                        }
                        else
                        {
                            <div class="kt-portlet__head-toolbar">
                                <div class="kt-portlet__head-wrapper">
                                    @if (Model.RoleId == 501)
                                    {
                                        if (Model.IsApproved == false && Model.IsRejected == false)
                                        {
                                            <a href="#" onclick="UpdateStatus(1)" class="btn btn-brand btn-elevate btn-icon-sm">
                                                <i class="la la-check"></i>
                                                Approve
                                            </a>
                                            <a href="#" onclick="UpdateStatus(2)" class="btn btn-danger btn-elevate btn-icon-sm">
                                                <i class="la la-close"></i>
                                                Reject
                                            </a>
                                        }
                                        else if (Model.IsApproved == true)
                                        {
                                            <i class="btn btn-brand btn-elevate btn-icon-sm">
                                                <i class="la la-check"></i>
                                                Approved
                                            </i>
                                        }
                                        else if (Model.IsRejected == true)
                                        {
                                            <i class="btn btn-danger btn-elevate btn-icon-sm">
                                                <i class="la la-close"></i>
                                                Rejected
                                            </i>
                                        }
                                    }
                                </div>
                            </div>
                            <a href="#" onclick="closeDetails()" class="details-close-button btn btn-danger btn-sm btn-icon btn-circle position-absolute">
                                <i class="la la-close"></i>
                            </a>
                        }
                    </div>
                    <div class="kt-portlet__body kt-portlet__body--fluid kt-portlet__body--fit">
                        <div class="kt-container  kt-grid__item kt-grid__item--fluid">
                            <div class="row">
                                <div class="col-md-12 pt-3 d-flex justify-content-between">
                                    <h4><small>Created By : </small>@Model.FullName</h4>
                                    <h4><small>Created On : </small>@Model.CreatedOn.ToString("dd-MM-yyyy HH:mm")</h4>
                                </div>
                                <div class="col-md-12">
                                    <form class="kt-form kt-form--label-right">
                                        <div class="kt-portlet__body all-details-view pt-1">
                                            <div class="row">
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Account</label>
                                                    <div>
                                                        <input class="form-control" type="text" value="@Model.AccountName" disabled>
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Product</label>
                                                    <div>
                                                        <select class="form-control kt-select2" id="ddlproductE" onchange="BindModels()" disabled>
                                                            @if (Model.ProductList != null)
                                                            {
                                                                foreach (var item in Model.ProductList)
                                                                {
                                                                    if (Model.ProductId == item.ProductId)
                                                                    {
                                                                        <option value="@item.ProductId" selected>@item.ProductName</option>
                                                                    }
                                                                    else
                                                                    {
                                                                        <option value="@item.ProductId">@item.ProductName</option>
                                                                    }
                                                                }
                                                            }
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">System ID</label>
                                                    <div>
                                                        <input class="form-control" type="text" value="@Model.SystemNo" disabled>
                                                    </div>
                                                </div>

                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Installation Date</label>
                                                    <div>
                                                        <input class="form-control" type="text" value="@Model.InstallationDate.ToString("dd-MM-yyyy")" disabled>
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Is Contract</label>
                                                    <div>
                                                        @if (Model.IsContract == true)
                                                        {
                                                            <p>Yes</p>
                                                            <input type="hidden" id="hdnIsContractType" value="1" />
                                                        }
                                                        else
                                                        {
                                                            <p>No</p>
                                                            <input type="hidden" id="hdnIsContractType" value="0" />
                                                        }
                                                        @*<input class="form-control" type="text" value="@Model.IsContract" disabled>*@
                                                    </div>
                                                </div>
                                                @if (Model.IsContract == true)
                                                {
                                                    <div class="form-group col-lg-6">
                                                        <label class="col-form-label">Contract Type</label>
                                                        <div>
                                                            <input class="form-control" type="text" value="@Model.ContractTypetxt" disabled>
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-lg-6">
                                                        <label class="col-form-label">PO Contract</label>
                                                        <div>
                                                            <input class="form-control" type="text" value="@Model.POContract" disabled>
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-lg-6">
                                                        <label class="col-form-label">Warranty Contract Expiry Date</label>
                                                        <div>
                                                            <input class="form-control" type="text" value="@Model.WarrantyExpiryDate.ToString("dd-MM-yyyy")" disabled>
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-lg-6">
                                                        <label class="col-form-label">PPM-Months</label>
                                                        <div>
                                                            <input class="form-control" type="text" value="@Model.PPMType" disabled>
                                                        </div>
                                                    </div>
                                                }
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <table class="table table-bordered mb-4">
                                                        <thead>
                                                            <tr>
                                                                <th>Model</th>
                                                                <th>Serial Number</th>
                                                                <th>Configuration</th>
                                                                <th>IP Address</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var item_ast in Model.AssetModelList)
                                                            {
                                                                <tr>
                                                                    <td>@item_ast.ModelName</td>
                                                                    <td>@item_ast.SerialNo</td>
                                                                    <td>@item_ast.Configuration</td>
                                                                    <td>@item_ast.IPAddress</td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="kt-portlet__head kt-portlet__head--lg">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon">
                                <i class="kt-font-brand flaticon2-line-chart"></i>
                            </span>
                            <h3 class="kt-portlet__head-title">
                                Editable
                            </h3>
                        </div>
                    </div>
                    <div class="kt-portlet__body kt-portlet__body--fluid kt-portlet__body--fit">
                        <div class="kt-container  kt-grid__item kt-grid__item--fluid">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="kt-portlet__body all-details-view">
                                        <form class="kt-form kt-form--label-right">
                                            <div class="row">
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Installation Date</label>
                                                    <div class="input-group date">
                                                        <input class="form-control" type="text" id="txtInstallationdate" value="@Model.InstallationDate.ToString("MM-dd-yyyy")">
                                                        <div class="input-group-append">
                                                            <span class="input-group-text">
                                                                <i class="la la-calendar-check-o"></i>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>

                                                @if (Model.IsContract == true)
                                                {
                                                    <div class="form-group col-lg-6">
                                                        <label class="col-form-label">PO Contract</label>
                                                        <div>
                                                            <textarea class="form-control" id="txtpocontractEdt">@Model.POContract</textarea>
                                                            @*<input  type="text" >*@
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-lg-6">
                                                        <label class="col-form-label">Warranty Contract Expiry Date</label>
                                                        <div class="input-group date">
                                                            <input class="form-control" type="text" id="txtwarrantydate" value="@Model.WarrantyExpiryDate.ToString("MM-dd-yyyy")">
                                                            <div class="input-group-append">
                                                                <span class="input-group-text">
                                                                    <i class="la la-calendar-check-o"></i>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                }
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Station Name</label>
                                                    <div>
                                                        <input class="form-control" id="txtstationNameE" type="text" value="@Model.StationName">
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Location</label>
                                                    <div>
                                                        <input class="form-control" id="txtlocationE" type="text" value="@Model.Area">
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Select Region</label>
                                                    <div>
                                                        <select class="form-control kt-select2" id="ddlregionE" onchange="BindCity()">
                                                            @if (Model.RegionList != null)
                                                            {
                                                                foreach (var item in Model.RegionList)
                                                                {
                                                                    if (Model.RegionId == item.RegionId)
                                                                    {
                                                                        <option value="@item.RegionId" selected>@item.RegionName</option>
                                                                    }
                                                                    else
                                                                    {
                                                                        <option value="@item.RegionId">@item.RegionName</option>
                                                                    }
                                                                }
                                                            }
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-6">
                                                    <label class="col-form-label">Select City</label>
                                                    <div>
                                                        <select class="form-control kt-select2" id="ddlcityE">
                                                            @if (Model.CityList != null)
                                                            {
                                                                foreach (var item in Model.CityList)
                                                                {
                                                                    if (Model.CityId == item.CityId)
                                                                    {
                                                                        <option value="@item.CityId" selected>@item.CityName</option>
                                                                    }
                                                                    else
                                                                    {
                                                                        <option value="@item.CityId">@item.CityName</option>
                                                                    }
                                                                }
                                                            }
                                                        </select>
                                                    </div>
                                                </div>
                                                @if (Model.RoleId == 502)
                                                {
                                                    <div class="form-group col-md-12">
                                                        <label>&nbsp;</label>
                                                        <div>
                                                            <a href="#" onclick="AddUpdatedDetails()" class="btn btn-success">Submit</a>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12 p-4 mb-4" style="border: 1px solid #ccc;">
                                                        <h3>Add Models</h3>
                                                        <div class="row">
                                                            <div class="col-lg-4">
                                                                <div class="form-group">
                                                                    <label class="col-form-label">Model</label>
                                                                    <div>
                                                                        <select class="form-control kt-select2" id="ddlModelE">
                                                                            @if (Model.RemainingModelList != null)
                                                                            {
                                                                                foreach (var item in Model.RemainingModelList)
                                                                                {
                                                                                    if (Model.ModelId == item.ModelId)
                                                                                    {
                                                                                        <option value="@item.ModelId" selected>@item.ModelName</option>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <option value="@item.ModelId">@item.ModelName</option>
                                                                                    }
                                                                                }
                                                                            }
                                                                        </select>
                                                                        <small id="spn_ddlModel" class="text-danger" style="display:none;">Field required.</small>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <div class="form-group">
                                                                    <label class="col-form-label">Configuration</label>
                                                                    <div>
                                                                        <input class="form-control" id="txtconfigurationE" type="text">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-4">
                                                                <div class="form-group">
                                                                    <label class="col-form-label">Serial Number</label>
                                                                    <div>
                                                                        <input class="form-control" type="text" id="txtSerialNumberE">
                                                                        <small id="spn_txtserialnumber" class="text-danger" style="display:none;">Field required.</small>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-12 mb-4">
                                                                <div class="form-group">
                                                                    <label class="col-form-label">IP Address</label>
                                                                    <div class="row">
                                                                        <div class="col-lg-3">
                                                                            <input class="form-control" type="text" id="txtipaddress1" value="" onkeypress="return isNumberKey(event)">
                                                                        </div>
                                                                        <div class="col-lg-3">
                                                                            <input class="form-control" type="text" id="txtipaddress2" value="" onkeypress="return isNumberKey(event)">
                                                                        </div>
                                                                        <div class="col-lg-3">
                                                                            <input class="form-control" type="text" id="txtipaddress3" value="" onkeypress="return isNumberKey(event)">
                                                                        </div>
                                                                        <div class="col-lg-3">
                                                                            <input class="form-control" type="text" id="txtipaddress4" value="" onkeypress="return isNumberKey(event)">
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-12">
                                                                <div class="form-group">
                                                                    <a href="#" onclick="AddMoreModels(@Model.AMId)" class="btn btn-info"><i class="la la-plus"></i> Add</a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <input type="hidden" id="hdnIpAddress" value="@Model.IPAddress" />
                                                }
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    @if (Model.PPMList.Count() > 0)
                    {
                        <div class="kt-portlet__body kt-portlet__body--fluid kt-portlet__body--fit">
                            <div class="kt-container  kt-grid__item kt-grid__item--fluid">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="kt-form kt-form--label-right">
                                            <div class="kt-portlet__body all-details-view">
                                                <div class="alert alert-info">
                                                    <strong>Note! </strong> Ticket Creation Option will be available for Expired and Upcoming Expired PPM.
                                                </div>
                                                <div class="table-responsive">
                                                    <table class="table table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th>PPM Date</th>
                                                                <th>Is Done</th>
                                                                <th>Actions</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var item in Model.PPMList)
                                                            {
                                                                <tr>
                                                                    <td>@item.isPPMDoneStr</td>
                                                                    <td>
                                                                        @if (item.isPPMDone == true)
                                                                        {
                                                                            <div class="kt-badge kt-badge--success kt-badge--inline">
                                                                                Yes
                                                                            </div>
                                                                        }
                                                                        else
                                                                        {
                                                                            <div class="kt-badge kt-badge--primary kt-badge--inline">
