                                <label>Next PPM Shedule</label>
                                <input type="text" class="form-control form-bottom-border" value="20-12-2019">
                            </div>
                            <div class="form-group col-xl-4 col-md-6">
                                <label>Customer Call Date & Time</label>
                                <input type="text" class="form-control form-bottom-border" value="05-12-2019 10:30 AM">
                            </div>
                            <div class="form-group col-xl-4 col-md-6">
                                <label>Service Start Date & Time</label>
                                <input type="text" class="form-control form-bottom-border" value="22-12-2019 12:00 PM">
                            </div>
                            <div class="form-group col-xl-4 col-md-6">
                                <label>Service End Date & Time</label>
                                <input type="text" class="form-control form-bottom-border" value="22-12-2025 11:59 PM">
                            </div>
                            <div class="form-group col-xl-4 col-md-6">
                                <label>Work Hours</label>
                                <input type="text" class="form-control form-bottom-border" value="8">
                            </div>
                            <div class="form-group col-xl-12">
                                <label>Total Charges</label>
                                <input type="text" class="form-control form-bottom-border" value="SAR 2020">
                            </div>
                            <div class="form-group col-xl-12">
                                <label>Report Type</label>
                                <input type="text" class="form-control form-bottom-border" value="Installation">
                            </div>
                            <div class="form-group col-xl-12">
                                <label for="exampleTextarea">Problem Description</label>
                                <textarea class="form-control" rows="3"></textarea>
                            </div>
                            <div class="form-group col-xl-12">
                                <label for="exampleTextarea">Activities Done</label>
                                <textarea class="form-control" rows="3"></textarea>
                            </div>
                            <hr />
                            <div class="form-group col-md-4">
                                <label>Name Of Engineer</label>
                                <h5>
                                    Vali Noor<br />
                                    <small>11-12-2019 04:00 PM</small>
                                </h5>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Name Of Engineer</label>
                                <h5>
                                    Vali Noor<br />
                                    <small>11-12-2019 04:00 PM</small>
                                </h5>
                            </div>
                            <div class="form-group col-md-4">
                                <label>Name Of Engineer</label>
                                <h5>
                                    Vali Noor<br />
                                    <small>11-12-2019 04:00 PM</small>
                                </h5>
                            </div>
